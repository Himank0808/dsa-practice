class Solution {
    public int maxProfit(int[] prices) {
        int maxp=0;
        int p=prices[0];
        for(int i=1;i<prices.length;i++){
          if(prices[i]<p){
            p=prices[i];
          }else{
            int max=prices[i]-p;
            maxp=Math.max(maxp,max);
          }
        }
       return maxp;
    }
}
